pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/Aparnadevi-lab/sql-wrapping.git'
            }
        }
        stage('Wrap SQL Files') {
            steps {
                sh 'python3 scripts/wrap_sql.py'
            }
        }
        stage('Archive Results') {
            steps {
                archiveArtifacts artifacts: 'output_sql/*', fingerprint: true
            }
        }
    }
}


